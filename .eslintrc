{
  "plugins": [
    "prettier"
  ],
  "extends": [
    "taro/react",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "prefer-const": "warn",
    "react/jsx-uses-react": "off",
    "react/react-in-jsx-scope": "off",
    "quotes": "off",
    "no-unused-vars": "off",
    "array-bracket-spacing": [
      "error",
      "never"
    ],
    "no-var": "warn",
    "block-spacing": [
      "warn",
      "never"
    ],
    "comma-spacing": [
      "error",
      {
        "before": false,
        "after": true
      }
    ],
    "no-cjs": "off",
    "@typescript-eslint/no-unused-vars": "error",
    "no-commonjs": "off"
  }
}